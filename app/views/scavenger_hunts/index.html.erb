<h1>Listing scavenger_hunts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scavenger_hunts.each do |scavenger_hunt| %>
  <tr>
    <td><%= scavenger_hunt.name %></td>
    <td><%= scavenger_hunt.start_date %></td>
    <td><%= scavenger_hunt.end_date %></td>
    <td><%= link_to 'Show', scavenger_hunt %></td>
    <td><%= link_to 'Edit', edit_scavenger_hunt_path(scavenger_hunt) %></td>
    <td><%= link_to 'Destroy', scavenger_hunt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Scavenger hunt', new_scavenger_hunt_path %>
